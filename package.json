{
  "name": "nginx-linter",
  "version": "0.1.0",
  "description": "A linter and formatter for Nginx configuration files",
  "bin": {
    "nginxlinter": "bin/nginxlinter.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:lint": "eslint bin lib test",
    "test:unit": "mocha test/*.spec.js test/**/*.spec.js"
  },
  "keywords": [
    "nginx",
    "linter"
  ],
  "author": "Jason Hinch",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "chalk": "^2.4.1",
    "pegjs": "^0.10.0",
    "table": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5"
  }
}
